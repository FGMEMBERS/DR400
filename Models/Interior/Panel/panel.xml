  <!--
    ########################################
    ##            ANIMATIONS              ##
    ########################################
  -->
  <!-- Trims wheel -->
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>rouetrim</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/flight/elevator-trim</property>
        <step>0.01</step>
      </binding>
    </action>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/flight/elevator-trim</property>
        <step>-0.01</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>rouetrim</object-name>
    <property>controls/flight/elevator-trim</property>
    <factor>-180</factor>
    <center>
      <x-m>  0.271 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.312 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Flaps handle -->
  <animation>
    <type>rotate</type>
    <object-name>flaps</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor> 45 </factor>
