  <!-- Levier des volets -->
  <animation>
    <type>rotate</type>
    <object-name>flaps</object-name>
    <property>/surface-positions/flap-pos-norm</property>
    <factor> 20 </factor>
    <center>
      <x-m>  0.718 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.300 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>flaps0</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-assign</command>
        <property>/controls/flight/flaps</property>
        <value type="double">0</value>
      </binding>          
    </action>
  </animation>
                                    
  <animation>
    <type>pick</type>
