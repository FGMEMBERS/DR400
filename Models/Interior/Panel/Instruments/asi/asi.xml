<?xml version="1.0"?>

<PropertyList>

  <path>asi.ac</path>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>PointerTip</object-name>
    <emission>
      <red>   1.0 </red>
      <green> 0.2 </green>
      <blue>  0.0 </blue>
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>PointerCentre</object-name>
    <object-name>PointerTip</object-name>
    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
     <entry><ind>  0 </ind><dep>   0 </dep></entry>
     <entry><ind> 55 </ind><dep>  45 </dep></entry>
     <entry><ind> 70 </ind><dep>  90 </dep></entry>
     <entry><ind> 90 </ind><dep> 145 </dep></entry>
     <entry><ind>110 </ind><dep> 190 </dep></entry>
     <entry><ind>130 </ind><dep> 240 </dep></entry>
     <entry><ind>150 </ind><dep> 290 </dep></entry>
     <entry><ind>170 </ind><dep> 335 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
  </animation>

</PropertyList>
