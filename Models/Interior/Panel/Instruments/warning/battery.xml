<?xml version="1.0"?>

<PropertyList>

    <path>on-off-R.ac</path>

    <animation>
        <!-- Objets opaques -->
        <object-name>on-off</object-name>
        <type>rotate</type>
        <property>/controls/electric/battery-switch</property>
        <factor>35</factor>
        <min-deg>0</min-deg>
        <max-deg>35</max-deg>
        <axis>
            <x>0.0</x>
            <y>-1.0</y>
            <z>0.0</z>
        </axis>

        <!-- Objets transparents -->
    </animation>
  
    <animation>
        <type>pick</type>
        <object-name>on-off</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>/controls/electric/battery-switch</property>
            </binding>
        </action>
    </animation>

</PropertyList>


