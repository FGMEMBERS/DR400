<?xml version="1.0"?>

<PropertyList>

  <path>charge.ac</path>

  <animation>
    <object-name>Face</object-name>
    <object-name>Needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>Needle</object-name>
    <emission>
      <red>   0.030 </red>
      <green> 0.015 </green>
      <blue>  0.005 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation> 
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property>/systems/electrical/volts</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>  0 </dep></entry> <!--   0 deg C -->
      <entry><ind> 40 </ind><dep> 65 </dep></entry> <!--  40 deg C -->
    </interpolation>
    <center>
      <x-m> -0.001 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.015 </z-m>  
    </center>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>


