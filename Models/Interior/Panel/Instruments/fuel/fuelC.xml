<?xml version="1.0"?>

<PropertyList>

  <path>fuelC.ac</path>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>Needle</object-name>
    <emission>
      <red>   1.0 </red>
      <green> 0.2 </green>
      <blue>  0.0 </blue>
      <factor-prop>/sim/model//material/instruments/factor</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property>consumables/fuel/tank[1]/level-lbs</property>
    <interpolation>
      <entry><ind>   0.00 </ind><dep>  5 </dep></entry>
      <entry><ind>  54.01 </ind><dep> 25 </dep></entry> <!--  35 litres -->
      <entry><ind>  92.59 </ind><dep> 45 </dep></entry> <!--  60 litres -->
      <entry><ind> 122.46 </ind><dep> 65 </dep></entry> <!--  80 litres -->
      <entry><ind> 154.32 </ind><dep> 85 </dep></entry> <!-- 100 litres -->
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.014 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>


