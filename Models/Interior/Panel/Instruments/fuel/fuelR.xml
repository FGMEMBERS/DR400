<?xml version="1.0"?>

<PropertyList>

  <path>fuelR.ac</path>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>Needle</object-name>
    <emission>
      <red>   0.030 </red>
      <green> 0.015 </green>
      <blue>  0.005 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property>consumables/fuel/tank[2]/level-lbs</property>
    <interpolation>
      <entry><ind>  0.00 </ind><dep>  5 </dep></entry>
      <entry><ind> 20.66 </ind><dep> 25 </dep></entry> <!-- 13 litres -->
      <entry><ind> 38.58 </ind><dep> 45 </dep></entry> <!-- 25 litres -->
      <entry><ind> 50.93 </ind><dep> 65 </dep></entry> <!-- 33 litres -->
      <entry><ind> 61.73 </ind><dep> 85 </dep></entry> <!-- 40 litres -->
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.014 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>


