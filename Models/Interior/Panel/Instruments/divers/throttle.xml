<?xml version="1.0"?>

<PropertyList>

  <path>manette.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>manette</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- Manette des gazs -->
  <animation>
    <type>translate</type>
    <object-name>manette</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <interpolation>
      <entry>
        <ind>  0.00 </ind>
        <dep>  0.10 </dep>
      </entry>
      <entry>
        <ind>  1.00 </ind>
        <dep>  0.00 </dep>
      </entry>
    </interpolation>
    <axis>
      <x> 1.0 </x>
      <y> 0.0 </y>
      <z> 0.0 </z>
    </axis>
  </animation>

</PropertyList>
