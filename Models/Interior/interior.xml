<?xml version="1.0"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>tour</object-name>
    <object-name>portefusibles</object-name>
    <object-name>sieges</object-name>
    <object-name>ceintures</object-name>
    <object-name>sono</object-name>
    <object-name>bout1</object-name>
    <object-name>bout2</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- Pilote -->
  <model>
    <name>Pilote</name>
    <path>Aircraft/DR400/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -1.474 </x-m>
      <y-m> -0.251 </y-m>
      <z-m> -0.029 </z-m>
    </offsets>
  </model>

  <!-- Tableau de bord -->
  <model>
    <path>Aircraft/DR400/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -1.859 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.099 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>  -5.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

  <!-- Les fusibles -->
  <model>
    <path>Aircraft/DR400/Models/Interior/Panel/Instruments/divers/full-fusibles.xml</path>
    <offsets>
      <x-m> 0.0 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.0 </z-m>
      <roll-deg>    0.0 </roll-deg>
      <pitch-deg>   0.0 </pitch-deg>
      <heading-deg> 0.0 </heading-deg>
    </offsets>
  </model>

</PropertyList>

